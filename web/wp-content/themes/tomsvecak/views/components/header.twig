<!DOCTYPE html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" {{ lang_options }}> <![endif]-->
<!--[if IE 7]>    <html class="no-js lt-ie9 lt-ie8" {{ lang_options }}> <![endif]-->
<!--[if IE 8]>    <html class="no-js lt-ie9" {{ lang_options }}> <![endif]-->
<!--[if IE 9]>    <html class="no-js lt-ie10" {{ lang_options }}> <![endif]-->
<!--[if gt IE 9]><!--> <html class="no-js" {{ lang_options }} ng-app="apastronNG"> <!--<![endif]-->
<head>
    <meta charset="{{site.charset}}" />
        <title>   
            {% if wp_title %}
                {{ wp_title }} - {{ site.name }}
            {% else %}
                {{ site.name }} - {{ site.description }}
            {% endif %}
        </title>
    <meta name="description" content="{{site.description}}">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="pingback" href="{{site.pingback_url}}" />
    <link rel="shortcut icon" href="{{site.theme.uri}}/favicon.png" />
    {{function('wp_head')}}
